<%= form_for @job do |f| %>
  <%= f.label :url %>
  <%= f.url_field :url %>
  <%= f.label :position %>
  <%= f.text_field :position %>
  <%= f.label :company %>
  <%= f.text_field :company %>
  <%= f.label :rating %>
  <%= f.text_field :rating %>
  <%= f.label :status %>
  <%= f.text_field :status %>
  <%= f.hidden_field :user_id, value: current_user.id %>
  <%= f.submit 'Add Job' %>
<% end %>
